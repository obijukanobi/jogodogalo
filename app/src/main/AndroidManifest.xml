<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.miguelreis.jogodogalo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:screenOrientation="portrait"
            android:name=".MainActivity"
            android:label="Jogo do Galo"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity
        android:name=".RegrasActivity"
        android:label="@string/menu_regras"
        >

        </activity>

        <activity
            android:name=".SettingsActivity"
            android:label="@string/settings"
            >
        </activity>
        <activity
            android:name=".UltimosJogosActivity"
            android:label="@string/menu_ultimos_jogos"
            >



        </activity>
        <activity
            android:name=".DetalhesJogosActivity"
            android:label="@string/last_games_details"
            >



        </activity>
    </application>

</manifest>
